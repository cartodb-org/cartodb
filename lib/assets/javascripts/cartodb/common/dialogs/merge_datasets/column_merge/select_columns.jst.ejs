<div class="MergeDatasets">
  <div class="js-sticky-header"></div>
  <div class="MergeDatasets-list">
    <div class="List List--selections">
      <div class="List-row">
        <div class="List-rowItem List-rowItem--compact List-rowItem--withoutBottomBorder List-rowItem--withoutBottomPadding js-left-table"></div>
      </div>
      <div class="List-row is-disabled">
        <div class="List-rowItem List-rowItem--compact">
          <div class="MergeDatasets-rowItem">
            <div class="MergeDatasets-rowItemRadio">
              <div class="RadioButton">
                <button class="RadioButton-input is-checked is-disabled" />
              </div>
            </div>
            <div>
              <% if (aliasUser) { %>
                <div class="DefaultTitle"><%- leftKeyColumnAlias %></div>
                <div class="DefaultTitle"><%- leftKeyColumnName %></div>
                <p class="DefaultParagraph DefaultParagraph--tertiary"><%- leftKeyColumnType %></p>  
              <% } else { %>
                <% if (leftKeyColumnAlias) { %>
                  <div class="DefaultTitle"><%- leftKeyColumnAlias %></div>
                  <p class="DefaultParagraph DefaultParagraph--tertiary"><%- leftKeyColumnType %></p>
                <% } else { %>
                  <div class="DefaultTitle"><%- leftKeyColumnName %></div>
                  <p class="DefaultParagraph DefaultParagraph--tertiary"><%- leftKeyColumnType %></p>
                <% } %>
              <% } %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="js-left-columns"></div>
  </div>

  <div class="MergeDatasets-list MergeDatasets-list--2nd">
    <div class="List List--selections">
      <div class="List-row">
        <div class="List-rowItem List-rowItem--compact List-rowItem--withoutBottomBorder List-rowItem--withoutBottomPadding js-right-table"></div>
      </div>
      <div class="List-row is-disabled">
        <div class="List-rowItem List-rowItem--compact">
          <div class="MergeDatasets-rowItem">
            <div class="MergeDatasets-rowItemRadio">
              <div class="RadioButton">
                <button class="RadioButton-input is-checked is-disabled" />
              </div>
            </div>
            <div>
              <% if (aliasUser) { %>
                <div class="DefaultTitle"><%- rightKeyColumnAlias %></div>
                <div class="DefaultTitle"><%- rightKeyColumnName %></div>
                <p class="DefaultParagraph DefaultParagraph--tertiary"><%- rightKeyColumnType %></p>  
              <% } else { %>
                <% if (rightKeyColumnAlias) { %>
                  <div class="DefaultTitle"><%- rightKeyColumnAlias %></div>
                  <p class="DefaultParagraph DefaultParagraph--tertiary"><%- rightKeyColumnType %></p>
                <% } else { %>
                  <div class="DefaultTitle"><%- rightKeyColumnName %></div>
                  <p class="DefaultParagraph DefaultParagraph--tertiary"><%- rightKeyColumnType %></p>
                <% } %>
              <% } %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="js-right-columns"></div>
  </div>
</div>